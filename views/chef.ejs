<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %> 


<body>
    <header>
        
        <%- include('./partials/nav.ejs') %> 
       
  
    <section>
       
        <div class="content">
            <button type="submit">CHEFS</button>

            <div class="container2">

                    <% if (signup.length > 0) { %>
                     <% signup.forEach(profile => { %>
                      
                        <div class="cards">
                    
                            <a href="/chef/<%= profile._id %> ">
                                <img src="/avatar/avatar<%= profile.avatar %> " alt="pic"> 
                            <h2><%= profile.firstName %> </h2>
                            <p><%= profile.email %> </p>
                        </a>
                        </div>

                
                        <% }) %>
                        <% }  else { %>  
                        <p><%= "No chefs to display currently" %> </p>
            
                        <% } %>  %> 
                
    
              </div>  
        </div>
    </section>

</header> 


   <%- include('./partials/footer.ejs') %> 

    
</body>
</html>